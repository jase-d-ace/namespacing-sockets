{
    "extends": "airbnb",
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["warn", "double"]
    },
    "env": {
      "es6": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
}
